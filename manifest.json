{
    "manifest_version": 3,
    "name": "PA Media and Design 2024",
    "version": "0.2",
    "action": {
        "default_title": "Click Me",
        "default_popup": "popup/index.html"
    },
    "options_page": "page/index.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/scrolling-timeout.js"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ]
}
